{
    "name": "statico/statico",
    "description": "Starter for the Statico flat file CMS",
    "type": "library",
    "require": {
        "asm89/twig-cache-extension": "^1.3",
        "filp/whoops": "^2.3",
        "itsgoingd/clockwork": "^4.0",
        "laminas/laminas-diactoros": "^2.1",
        "laminas/laminas-escaper": "^2.6",
        "laminas/laminas-form": "^2.14",
        "laminas/laminas-httphandlerrunner": "^1.0",
        "laminas/laminas-i18n": "^2.9",
        "laminas/laminas-view": "^2.11",
        "league/container": "^3.2",
        "league/event": "^2.2",
        "league/flysystem": "^1.0",
        "league/flysystem-cached-adapter": "^1.0",
        "league/flysystem-memory": "^1.0",
        "league/glide": "^1.5",
        "league/route": "^4.2",
        "mnapoli/front-yaml": "^1.6",
        "monolog/monolog": "^2.0",
        "psy/psysh": "^0.9.9",
        "publishing-kit/cache": "^0.0.1",
        "publishing-kit/config": "^0.0.5",
        "publishing-kit/csrf": "^0.0.2",
        "publishing-kit/http-proxy": "^0.0.1",
        "publishing-kit/utilities": "^0.0.1",
        "symfony/console": "^4.3",
        "symfony/http-foundation": "^4.3",
        "symfony/mailer": "^4.3",
        "tedivm/stash": "^0.15.1",
        "twig/twig": "^2.6",
        "vlucas/phpdotenv": "^2.5"
    },
    "require-dev": {
        "doctrine/orm": "^2.6",
        "infection/infection": "^0.15.3",
        "laminas/laminas-db": "^2.11",
        "league/flysystem-aws-s3-v3": "^1.0",
        "league/flysystem-azure-blob-storage": "^0.1.5",
        "league/flysystem-sftp": "^1.0",
        "mockery/mockery": "^1.2",
        "php-coveralls/php-coveralls": "^2.1",
        "php-mock/php-mock-mockery": "^1.3",
        "phpmd/phpmd": "^2.6",
        "phpunit/phpunit": "^8.5",
        "sebastian/phpcpd": "^4.1",
        "spatie/flysystem-dropbox": "^1.0",
        "squizlabs/php_codesniffer": "^3.5",
        "vimeo/psalm": "^3.10"
    },
    "suggest": {
        "spatie/flysystem-dropbox": "Allows use of Dropbox as the Flysystem backend"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Matthew Daly",
            "email": "450801+matthewbdaly@users.noreply.github.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Statico\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    }
}
