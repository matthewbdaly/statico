{
    "name": "statico/statico",
    "description": "Starter for the Statico flat file CMS",
    "type": "library",
    "require": {
        "asm89/twig-cache-extension": "^1.3",
        "filp/whoops": "^2.3",
        "itsgoingd/clockwork": "^3.1",
        "league/container": "^3.2",
        "league/event": "^2.2",
        "league/flysystem": "^1.0",
        "league/flysystem-cached-adapter": "^1.0",
        "league/glide": "^1.5",
        "league/route": "^4.2",
        "mnapoli/front-yaml": "^1.6",
        "monolog/monolog": "^1.24",
        "symfony/console": "^4.2",
        "symfony/http-foundation": "^4.3",
        "symfony/mailer": "^4.3",
        "symfony/yaml": "^4.2",
        "tedivm/stash": "^0.15.1",
        "twig/twig": "^2.6",
        "vlucas/phpdotenv": "^2.5",
        "zendframework/zend-config": "^3.2",
        "zendframework/zend-diactoros": "^2.1",
        "zendframework/zend-escaper": "^2.6",
        "zendframework/zend-form": "^2.14",
        "zendframework/zend-httphandlerrunner": "^1.0",
        "zendframework/zend-i18n": "^2.9",
        "zendframework/zend-mail": "^2.10",
        "zendframework/zend-view": "^2.11"
    },
    "require-dev": {
        "infection/infection": "^0.12.2",
        "league/flysystem-aws-s3-v3": "^1.0",
        "league/flysystem-azure-blob-storage": "^0.1.5",
        "league/flysystem-memory": "^1.0",
        "league/flysystem-sftp": "^1.0",
        "mockery/mockery": "^1.2",
        "php-coveralls/php-coveralls": "^2.1",
        "php-mock/php-mock-mockery": "^1.3",
        "phpmd/phpmd": "^2.6",
        "phpstan/phpstan": "^0.10.7",
        "phpunit/phpunit": "^7.5",
        "psy/psysh": "^0.9.9",
        "spatie/flysystem-dropbox": "^1.0",
        "squizlabs/php_codesniffer": "^3.4",
        "vimeo/psalm": "^3.4"
    },
    "suggest": {
        "spatie/flysystem-dropbox": "Allows use of Dropbox as the Flysystem backend"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Matthew Daly",
            "email": "450801+matthewbdaly@users.noreply.github.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Statico\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    }
}
